{
  "functions": {},
  "outputDirectory": null,
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://blog.mellon.design"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog.mellon.design/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.mellon.design",
      "permanent": true
    },
    {
      "source": "/:path*",
      "destination": "https://www.mellon.design/:path*",
      "permanent": true
    }
  ]
}
